	}

.meetings .media-object{
	width:175px;
}
	.meetings .media-object h2{
		text-align:left;
	}
	
	.meetings .media-body{
		font-size:18px;
		font-family:'HelveticaLTStd-Light';
		font-weight:normal;

	}
	
	@media screen and (min-width: 768px) and (max-width: 991px)
	{
		.meetings .media-body{
			line-height:53px;
		}
	}

